{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run SpecFuzzer backend",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/backend/.venv/bin/python",
      "args": [
        "-m",
        "uvicorn",
        "backend.main:app",
        "--host",
        "127.0.0.1",
        "--port",
        "8000",
        "--reload"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
